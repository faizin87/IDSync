<!DOCTYPE html>
@{ 
    string webApi = System.Configuration.ConfigurationManager.AppSettings["ids:WebApi"];
}
<html id="xamzo">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/Images/icons/favicon.png" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css") 
    @Styles.Render("~/Content/tree") 
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jsupload")
    <link href="@webApi/SFB/styles/chat.css" rel="stylesheet" type="text/css" />
    <link href="@webApi/SFB/styles/mfp.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        var baseApi = '@webApi';
        var domain = '@IDSync.Configs.MainConfig.Conf("AdDomain")';
        var config = {
            version: 'BuildConference/1.0.0', // this helps to identify telemetry generated by the samples
            apiKey: 'a42fcebd-5b43-4b89-a065-74450fb91255', // SDK DF
            apiKeyCC: '9c967f6b-a846-4df2-b43d-5167e47d81e1' // SDK+CC DF
        };
    </script>
    <script src="https://swx.cdn.skype.com/shared/v/1.2.15/SkypeBootstrap.min.js"></script>
    
</head>
<body>
    <audio id="chatAudio">
        <source src="@webApi/NodeJs/sounds/notify.ogg" type="audio/ogg">
        <source src="@webApi/NodeJs/sounds/notify.mp3" type="audio/mpeg">
        <source src="@webApi/NodeJs/sounds/notify.wav" type="audio/wav">
    </audio> 
    <!--socket flash notice-->
    <div id="flash"></div>
    <div id="wrapper">
        @Html.Partial("_Menu")
        <div id="syncinprocess" style="display:none"><p class="label label-warning-light">Syncronizing...</p></div>
        <div id="page-wrapper" class="gray-bg dashbard-1">
            @Html.Partial("_Header") 
            <div id="full-awesomesearch"></div>
            <div class="x-right">@RenderBody()</div>
            <div>&nbsp;</div>
            @Html.Partial("_Footer")
        </div>
        @if (IDSync.Configs.MainConfig.Conf("EnableSkype").ToString() == "true")
        {
            @Html.Partial("_Lyncs");
        } 
    </div>  
    <script src="@webApi/NodeJs/md5.min.js"></script>
    <script src="https://js.bkn.go.id/socket.io/socket.io.js"></script>
    <script src="~/NodeJs/client.js"></script> 
        @Scripts.Render("~/bundles/plugins")
        @Scripts.Render("~/bundles/tree") 
    @if (IDSync.Configs.MainConfig.Conf("EnableSkype").ToString() == "true")
    {
        <script src="@webApi/SFB/scripts/index.js"></script>
        <script src="@webApi/SFB/scripts/sign-in.js"></script>
        <script src="@webApi/SFB/scripts/sign-out.js"></script>
        <script src="@webApi/SFB/scripts/self.js"></script>
        <script src="@webApi/SFB/scripts/chat-service.js"></script>
        <script src="@webApi/SFB/app/chat.js"></script>
        <script src="@webApi/SFB/app/jquery.mfp.min.js"></script>
        <script src="@webApi/SFB/app/sound.js"></script>
        <script src="@webApi/SFB/app/socket.js"></script>
        <script src="@webApi/SFB/app/utillity.js"></script>
    } 
</body>
</html>
